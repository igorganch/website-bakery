    <div class = "page">
            <div class="dashcontent-update">
                <div class = "header-update">
                    <div class = "divider-header">

                    <div class = "circle ">

                    </div>
                    <div class = "header-options-logo"> 
                        Visitors
                    </div>



                    </div> 

                    
                </div> 
                <div class = "dashboard-uppersection">



                <div class = "visitors">
                    <div class = "feature-content">
                        <div class = "upper-section white">
                            V i s i t o r s 
                        </div>
                        <div class="mid-section-total">
                            <div id= "24-hour-visitors"class = "mid-Section white">
                            
                            </div>
                            <div id ="precentage" class = "mid-lower-Section ">
                            
                            </div>

                        </div>                       
                         <div class = "lower-section white">
                            I n &nbsp;&nbsp; t h e &nbsp;&nbsp; l a s t &nbsp;&nbsp; 2 4 &nbsp;&nbsp; h o u r s
                        </div>

                    </div>
                </div>
                                <div class = "orders">
                    <div class = "feature-content">
                        <div class = "upper-section black">
                            V I S I T O R S
                        </div>
                        <div class="mid-section-total">
                            <div id = "live-counter" class = "mid-Section black ">
                        
                            </div>


                        </div>                       
                         <div class = "lower-section black">
                           C u r r e n t l y 
                        </div>

                    </div>
                </div>

                <div class = "orders">
                    <div class = "feature-content">
                        <div class = "upper-section black">
                            C U S T O M E R S
                        </div>
                        <div class="mid-section-total">
                            <div class = "mid-Section black ">
                            34
                            </div>
                            <div class = "mid-lower-Section ">
                            + 54%
                            </div>

                        </div>                       
                         <div class = "lower-section black">
                            I n &nbsp;&nbsp; t h e &nbsp;&nbsp; l a s t &nbsp;&nbsp; 2 4 &nbsp;&nbsp; h o u r s
                        </div>

                    </div>
                </div>

                </div>
                
                <div class="dashchart">
                        <div class ="table-header-full">
                            <div class = "table-header-upper"> V I S I T O R S</div>
                            <div class="table-header-all">
  
                                <div class="table-header center-text">Email</div>
                                <div class="table-header center-text">Location</div>  
                                <div class="table-header center-text">Status</div>
                                <div class="table-header center-text">Page</div>
                                <div class="table-header center-text">IP</div>
                                <div class="table-header-userAgent center-text">User Agent</div>
                                <div class="table-header-userAgent center-text">Last Activity</div>
                                <div class="table-header-products-start-end end-text"></div>
                            </div>
                        </div>

                    <div class="login-rows">
                        {{#each user}}
                        <div id = '{{this.dataValues.CartCookie}}-cookie' class="each-user">

                            <div id='{{this.dataValues.CartCookie}}-email' class="equal-log-columns center-text ">
                                {{#if this.dataValues.email}}
                                    {{this.dataValues.email}}
                                {{else}}
                                    N/A
                                {{/if}}
                                </div>
                            <div id='{{this.dataValues.CartCookie}}-location' class="equal-log-columns center-text password">
                                {{this.dataValues.location}}
                              </div>
                            {{#if this.dataValues.status}}   
                            <div id='{{this.dataValues.CartCookie}}-status' class="equal-log-columns center-text active status">
                              Online </div>
                              {{else}}
                            <div id='{{this.dataValues.CartCookie}}-status' class="equal-log-columns center-text non-active status">
                              Offline</div>
                              {{/if}}
                            <div id='{{this.dataValues.CartCookie}}-page' class="equal-log-columns center-text page-loc">
                              {{this.dataValues.page}}</div>
                            <div id='{{this.dataValues.CartCookie}}-ip' class="equal-log-columns center-text address">
                                {{this.dataValues.ip}}</div>
                            <div id='{{this.dataValues.CartCookie}}-userAgent' class="center-text userAgent">
                                {{this.dataValues.userAgent}}</div>
                            <div id='{{this.dataValues.CartCookie}}-updatedAt' class="equal-log-columns center-text userAgent">
                                    {{this.dataValues.updatedAt}}
                            </div>
                            <div class="equal-log-columns-products-start-end center-text address">
                                <i id = '{{this.dataValues.CartCookie}}-edit'class="edit-button fa-solid fa-pen-to-square changecolor"></i>
                            </div>
                            
                        </div>
                        {{/each}}
                    </div>
                        

                    </div>
                
            </div>


    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="/static/dashboardVisitors.js"></script>
    </body>
    </html>